//Problem-03 
#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    int ans = 0;
    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        ans = ans ^ x; 
    }

    cout << ans << endl;
    return 0;
}

// Input: 7
//        1 1 2 2 3 4 4

//Output: 3
