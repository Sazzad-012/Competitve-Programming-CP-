#include <bits/stdc++.h>
using namespace std;

bool isUgly(int n) {
    if (n <= 0) return false;

    for (int factor : {2, 3, 5}) {
        while (n % factor == 0) {
            n /= factor;
        }
    }
    return n == 1;
}

int main() {
    int n;
    cin >> n;

    int count = 0;
    int current = 0;

    while (count < n) {
        ++current;
        if (isUgly(current)) {
            ++count;
        }
    }

    cout << current << endl;
    return 0;
}
