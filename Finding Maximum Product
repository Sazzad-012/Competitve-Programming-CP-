#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    int arr[n];

    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    long long maxProduct = -1e18; 

    
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            long long product = 1LL * arr[i] * arr[j];
            if (product > maxProduct) {
                maxProduct = product;
            }
        }
    }

    cout << maxProduct << endl;
    return 0;
}

//Input: 5
//       5 6 9 0 1

//Output: 54
