#include <bits/stdc++.h>
using namespace std;

int climbStairs(int n) {
    long total = 0;
    int a = 0, b = 1;
    for (int i = 0; i < n; i++) {
        total = a + b;
        a = b;
        b = total;
    }
    return total;
}

int main() {
    int n;
    cin >> n;

    int ways = climbStairs(n);
    cout << n << "  " << endl;

    return 0;
}
