#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    
    long long total_sum = (long long)n * (n + 1) / 2;
    long long given_sum = 0;
    for (int i = 0; i < n - 1; i++) {
        long long x;
        cin >> x;
        given_sum += x;
    }
    
    cout << total_sum - given_sum << endl;
    
    return 0;
}
