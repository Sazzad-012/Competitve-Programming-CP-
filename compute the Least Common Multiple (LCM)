#include <bits/stdc++.h>
using namespace std;

int main() {
    int a, b;
    cin >> a >> b;

    int lcm;
    int maxNum = (a > b) ? a : b;

    for (int i = maxNum; ; i++) {
        if (i % a == 0 && i % b == 0) {
            lcm = i;
            break;
        }
    }

    cout << "a = " << a << ", b = " << b << endl;
    cout << "LCM: " << lcm << endl;

    return 0;
}
